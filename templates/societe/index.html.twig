<td>
<a href= "{{path('societe_update' ,{'id':sos.id})}}">Update </a>
</td>
<td>
<a href= "{{path('remove_sos' ,{'id':sos.id})}}">Remove </a>
</td>
</tr>
{%endfor%}
</tbody>
</table>
</div>
{% endblock %}